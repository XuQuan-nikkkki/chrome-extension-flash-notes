{
  "name": "Flash notes",
  "description": "Transcribes speech to text, let you focus on your thoughts",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {},
  "icons": 
    {
      "16": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png",
      "512": "favicon.png"
    },
  "permissions": [
    "tabs"
  ]
}
